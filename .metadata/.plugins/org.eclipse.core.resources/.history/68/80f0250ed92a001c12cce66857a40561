
public class Hellpers {
	public static void main(String[] args) {
		// declaração de variaveis a ser utilizada
		int n1 = 8, n2 = 0, n3 = 0;

		for (int i = 1; n1 <= 100; i++) {
			System.out.println(n1);
			n2 = n1 / 2;
			System.out.println(n2);
			n3 = n2 * 3;
			System.out.println(n3);
		

		}
	}
}
