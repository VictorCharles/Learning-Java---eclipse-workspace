public class Hellpers {
	public static void main(String[] args) {
		// defina tamanho de caracteres por linha, e depois de quantas linhas (por
		// padrão 10 em cada)

		int tamanhoLinha = 6, qtdlinhas = 6, n = 0;
		for (int i = 1; i < tamanhoLinha; i++) {
			for (int li = 1; li < tamanhoLinha; li++) {
				if (n == 1) {
					System.out.print("1");
					n = 0;
					break;
				}
				if (n == 0) {
					System.out.print("0");
					n = 1;
					break;
				}
			}
			System.out.println();
		}
	}
}
