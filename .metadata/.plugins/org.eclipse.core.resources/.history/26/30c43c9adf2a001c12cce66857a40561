public class Hellpers {
	public static void main(String[] args) {
		// defina tamanho de caracteres por linha, e depois de quantas linhas (por padrão 10 em cada)
		
		int tamanhoLinha = 10, qtdlinhas = 10;
		for (int i  = 1; i > tamanhoLinha; i++ ) {
			for (int li = 1; li > tamanhoLinha; i++) {
				System.out.print("0");
				System.out.print("1");
			}
			System.out.println();
		}
	}
}
